{
  "name": "vision2020",
  "version": "0.1.0",
  "private": true,
  "main": "main/index.js",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build && next export",
    "electron": "electron .",
    "dist": "npm run build && electron-builder --dir",
    "static": "npx serve -p 3010 out"
  },
  "build": {
    "asar": false,
    "extraResources": [
      {
        "from": "out",
        "to": "app/renderer"
      }
    ]
  },
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "csv2json": "2.0.2",
    "electron": "7.1.7",
    "electron-builder": "22.1.0",
    "electron-rebuild": "1.8.8",
    "eslint-config-standard": "14.1.0",
    "eslint-config-standard-react": "9.2.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "eslint-plugin-standard": "4.0.1",
    "husky": ">=3",
    "lint-staged": ">=9",
    "prettier": "1.19.1"
  },
  "dependencies": {
    "@react-spring/parallax": "9.0.0-canary.808.8.4a87fcb",
    "@zeit/next-css": "1.0.1",
    "electron-is-dev": "1.1.0",
    "electron-next": "3.1.5",
    "eslint": "6.8.0",
    "firebase": "7.6.1",
    "image-trace-loader": "1.0.2",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-optipng": "7.1.0",
    "imagemin-svgo": "7.0.0",
    "jimp": "0.9.3",
    "lodash": "4.17.15",
    "next": "9.1.6",
    "next-compose-plugins": "2.2.0",
    "next-optimized-images": "2.5.4",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-image-gallery": "1.0.3",
    "react-intersection-observer": "8.25.2",
    "react-markdown": "4.2.2",
    "react-spring": "8.0.27",
    "responsive-loader": "1.2.0",
    "webp-loader": "0.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,scss,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
